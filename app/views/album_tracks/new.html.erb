<% @page_title = "New music album track" %>
<%= link_to("back to album" , artist_music_group_album_path(@artist,@group,@album))%>
<%= error_messages_for(@track) %>


<%= form_for([@artist,@group,@album,@track] , html: { multipart: true } ) do |f| %>

	<%= render(:partial => "album_tracks/form" , :locals => {:f => f , :t => @track_length}) %>
	<%= submit_tag("Create new album track" , :class=> "btn btn-primary") %>
<% end %>