<% @page_title = "New music group album" %>
<%= link_to("back to albums" , artist_music_group_albums_path(@artist,@owner))%>
<%= error_messages_for(@album) %>


<%= form_for([@artist,@owner,@album], :html => { multipart: true }) do |f| %>

	<%= render(:partial => "albums/form" , :locals => {:f => f}) %>
	<%= submit_tag("edit current album", :class=> "btn btn-primary") %>
<% end %>